<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.simpletime.pojo">
    <class name="Book" table="book">
       <id name="id" type="string" column="id">
            <generator class="uuid"/>
       </id>
       <property name="name" type="java.lang.String" column="name"/>
       <property name="contentSynopsis" type="java.lang.String" column="contentSynopsis"/>
       <property name="author" type="java.lang.String" column="author" />
       <property name="photoUrl" type="java.lang.String" column="photoUrl" />
       <property name="tryReadContent" type="java.lang.String" column="tryReadContent" />
       <property name="authorSynopsis" type="java.lang.String" column="authorSynopsis" />
       <property name="price" type="java.lang.Float" column="price" />
       <property name="score" type="java.lang.Float" column="score" />
       <property name="uploadUserId" type="java.lang.String" column="uploadUserId" />
       <property name="buileTime" type="java.util.Date" column="buileTime" />
       <property name="valid" type="java.lang.Boolean" column="valid" />
       <property name="countPraise" type="java.lang.Integer" column="countPraise" />
       <property name="countComment" type="java.lang.Integer" column="countComment" />
       <property name="countBuy" type="java.lang.Integer" column="countBuy" />
       <property name="isbn" type="java.lang.String" column="isbn"/>
    </class>
</hibernate-mapping>
